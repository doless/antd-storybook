{
  "name": "antdesign-storybook",
  "version": "0.1.0",
  "description": "Stories to run quickly with Ant Design UI components based storybook",
  "author": "Himoriarty",
  "repository": {
    "type": "git",
    "url": " https://github.com/doless/antd-storybook"
  },
  "bugs": {
    "url": " https://github.com/doless/antd-storybook/issues"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@babel/helper-builder-react-jsx": "^7.14.5",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.16.12",
    "antd-img-crop": "^3.14.3",
    "fetch-jsonp": "^1.2.1",
    "immutability-helper": "^3.1.1",
    "lodash": "^4.17.21",
    "rc-tween-one": "^2.7.3",
    "react": "^17.0.1",
    "react-dnd": "^14.0.3",
    "react-dnd-html5-backend": "^14.0.1",
    "react-dom": "^17.0.1",
    "react-highlight-words": "^0.17.0",
    "react-infinite-scroller": "^1.2.4",
    "react-resizable": "^3.0.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-sortable-hoc": "^2.0.0",
    "react-sticky": "^6.0.3",
    "react-text-loop": "^2.3.0",
    "react-virtualized": "^9.22.3",
    "react-window": "^1.8.6",
    "reqwest": "^2.0.5",
    "typescript": "^4.3.5",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.0",
    "@storybook/addon-essentials": "^6.3.0",
    "@storybook/addon-links": "^6.3.0",
    "@storybook/node-logger": "^6.3.0",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.3.0",
    "@storybook/testing-react": "^0.0.17"
  }
}
